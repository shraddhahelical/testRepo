			 WHEN SubTransCode = 'P042' THEN 'P042 ACCS- Payt. TempHardship(IHC)'
			 WHEN SubTransCode = 'P043' THEN 'P043 ACCS- Payt. Trans.ToWork(IHC)'
			 WHEN SubTransCode = 'P060' THEN 'P060 ACCS- Payt. One Off Payment'
			 WHEN SubTransCode = '4030' THEN '4030 Reversal'
			 WHEN SubTransCode = '0100' THEN '0100 PmtOnAcct'
