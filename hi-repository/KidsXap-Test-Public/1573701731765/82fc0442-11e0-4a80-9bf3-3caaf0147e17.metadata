			 WHEN SubTransCode BETWEEN 'R001' AND 'R119' THEN SubTransCode + ' ' + 'Debt Repayment'
			 ELSE SubTransCode END AS [Sub Trans Code]
	  , CASE WHEN MainTransCode = '01' THEN '01 Incoming Payment'
			 WHEN MainTransCode = '02' THEN '02 Outgoing Payment'
			 WHEN MainTransCode = '08' THEN '08 Account Maintenance (OFFSET)'
			 WHEN MainTransCode = '10' THEN '10 Returns'
			 WHEN MainTransCode = '15' THEN '15 Automatic Clearing (OFFSET)'
			 WHEN MainTransCode = 'Z3' THEN 'Z3 Withholdings'
		ELSE ClearingReasonName END AS [Clearing Reason]
