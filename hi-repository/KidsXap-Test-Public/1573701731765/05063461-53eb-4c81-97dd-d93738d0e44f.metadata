            <view id="a58650bd-02e7-49e1-b40d-de447068cee8" name="DietRequirementView" alias="DietRequirementView">
                <query><![CDATA[	SELECT 
	DISTINCT ISNULL(Health.HealthTag, '-') AS [Dietary], 
	Health.OtherDetails AS [Diet Requirement],
	Book.FamilyId, 
	Family.FamilyName AS [Family],
	Book.ChildId, 
	Child.ChildName AS [Child],
	Book.RoomName AS [Rooms],
	Family.FamilyStatus AS [Family Status], 
	Child.ChildFamilyStatus AS [Child Family Status],
	Family.OrganisationId, 
	Family.OrganisationName AS [Organisation], 
	Family.ProviderId, 
	Family.ProviderName AS [Provider], 
	Family.CentreId, 
	Family.CentreName AS [Centre], 
	Book.BookingStatus AS [Booking Status],
	 Book.BookingDate,
	 
	CASE WHEN (SELECT ISNULL(COUNT(Attached.Id), 0) FROM dbo.rpt_FamilyAttachments Attached WHERE Attached.AttachmentId IN ('64b6b4a1-f458-4817-8ddc-1faec2b03c28') AND
				Attached.PersonId = Child.ChildId) >  0 THEN 'Y'
	     WHEN (SELECT ISNULL(COUNT(Attached.Id), 0) FROM dbo.rpt_FamilyAttachments Attached WHERE Attached.AttachmentId IN ('64b6b4a1-f458-4817-8ddc-1faec2b03c28') AND
				Attached.PersonId = Child.ChildId) =  0  THEN 'N' ELSE '-' END AS [Document Uploaded]

	FROM [dbo].[rpt_ChildHealthDetails] Health 
	INNER JOIN [dbo].[rpt_FamilyChild] Child ON Health.PersonId = Child.ChildId
	INNER JOIN [dbo].rpt_Family Family ON Family.FamilyId = Child.FamilyId
	INNER JOIN [dbo].[rpt_Bookings] Book ON Book.FamilyId = Family.FamilyId AND Book.ChildId = Child.ChildId
	WHERE ISNULL(Health.HealthTag, '-') = 'DietaryRestrictions'
		/*Book.BookingDate >= DATEADD(week, DATEDIFF(day, 0, GETUTCDATE())/7, 0) first day current week
        AND Book.BookingDate <=  DATEADD(week, DATEDIFF(day, 0, GETUTCDATE())/7, 6) last day current week*/]]></query>
            </view>
            <view id="80cb1a4d-5dea-4b60-93ef-6b9d89676f03" name="ChildImmunisationView" alias="ChildImmunisationView">
                <query><![CDATA[SELECT
	Family.[CentreName] AS [Centre],
 	Family.[OrganisationName]  AS [Organisation],
	Family.ProviderName AS [Provider],
 	Child.ChildName AS [Child],
	Family.[FamilyName] AS [Family],
	COALESCE(CONVERT(VARCHAR, cd.DateOfBirth, 103), '-') AS [Child DOB],
	DATEDIFF(MONTH, DateOfBirth, CONVERT(DATE, DATEADD(HOUR, 11, GETUTCDATE()))) AS [Child Age-Mth],

	CASE WHEN cd.IsImmunised = 1 THEN 'Y'
	     WHEN cd.IsImmunised = 0 THEN 'N'
		 WHEN cd.IsImmunised IS NULL THEN 'Not Answered'  END AS [Immunised],

	CASE WHEN cd.ImmunisationDocumentType IS NULL THEN '-'
		 WHEN (cd.ImmunisationDocumentType IS NOT NULL AND (SELECT ISNULL(COUNT(Attached.Id), 0) FROM dbo.rpt_FamilyAttachments Attached WHERE Attached.AttachmentId IN ('58118d4f-f6d1-417c-8234-59cdbda69aed','07c56326-bcbc-45a7-aaa3-14b8e2193593') AND
				Attached.PersonId = cd.PersonId) >  0 ) THEN 'Y'
	     WHEN (cd.ImmunisationDocumentType IS NOT NULL AND (SELECT ISNULL(COUNT(Attached.Id), 0) FROM dbo.rpt_FamilyAttachments Attached WHERE Attached.AttachmentId IN ('58118d4f-f6d1-417c-8234-59cdbda69aed','07c56326-bcbc-45a7-aaa3-14b8e2193593') AND
				Attached.PersonId = cd.PersonId) =  0 ) THEN 'N' END AS [Document Uploaded],
	
	/*IIF(cd.ImmunisationDocumentType IS NULL,  
				CASE WHEN (SELECT ISNULL(COUNT(Attached.Id), 0) FROM dbo.rpt_FamilyAttachments Attached WHERE Attached.AttachmentId IN ('8b71dfc7-6f3c-47ae-b757-4408f1d27fe3') AND
				Attached.PersonId = cd.PersonId) >  0 THEN 'Medical Exemption' ELSE '-' END, ISNULL(cd.ImmunisationDocumentType, '-')) AS [Document Type],*/
	ISNULL(cd.ImmunisationDocumentType, '-') AS [Document Type],

	CASE WHEN cd.IsBirth = 1 THEN 'Y'
	     WHEN cd.IsBirth = 0 THEN 'N'
		 WHEN cd.IsBirth IS NULL THEN '-' END AS [Birth],

	CASE WHEN cd.Is2MonthsOfAge = 1 THEN 'Y'
	     WHEN cd.Is2MonthsOfAge = 0 THEN 'N'
		 WHEN cd.Is2MonthsOfAge IS NULL THEN '-' END AS [2 Months],
	
	CASE WHEN cd.Is4MonthsOfAge = 1 THEN 'Y'
	     WHEN cd.Is4MonthsOfAge = 0 THEN 'N'
		 WHEN cd.Is4MonthsOfAge IS NULL THEN '-' END AS [4 Months],
	
	CASE WHEN cd.Is6MonthsOfAge = 1 THEN 'Y'
	     WHEN cd.Is6MonthsOfAge = 0 THEN 'N'
		 WHEN cd.Is6MonthsOfAge IS NULL THEN '-' END AS [6 Months],
	
	CASE WHEN cd.Is12MonthsOfAge = 1 THEN 'Y'
	     WHEN cd.Is12MonthsOfAge = 0 THEN 'N'
		 WHEN cd.Is12MonthsOfAge IS NULL THEN '-' END AS [12 Months],

	CASE WHEN cd.Is18MonthsOfAge = 1 THEN 'Y'
	     WHEN cd.Is18MonthsOfAge = 0 THEN 'N'
		 WHEN cd.Is18MonthsOfAge IS NULL THEN '-' END AS [18 Months],

	CASE WHEN cd.Is4YearsOfAge = 1 THEN 'Y'
	     WHEN cd.Is4YearsOfAge = 0 THEN 'N'
		 WHEN cd.Is4YearsOfAge IS NULL THEN '-' END AS [4 Years],

	CASE WHEN cd.Is6MonthsTo5YearsOfAge = 1 THEN 'Y'
	     WHEN cd.Is6MonthsTo5YearsOfAge = 0 THEN 'N'
		 WHEN cd.Is6MonthsTo5YearsOfAge IS NULL THEN '-' END AS [6 Months to 5 Years],

	CONVERT(VARCHAR(MAX), ISNULL((STUFF((SELECT DISTINCT ', ' + CONVERT(NVARCHAR(MAX), book.RoomName) FROM
	dbo.rpt_Bookings book WHERE book.ChildId = Child.ChildId AND book.BookingStatus = 'Approved' FOR XML PATH('')) , 1, 2, '')), '-')) AS Rooms,
	Book.RoomName,
	Book.BookingDate AS [Booking Date],

	ISNULL(Child.ChildFamilyStatus, '-') AS [Child Status],
	ISNULL(Family.FamilyStatus, '-') AS [Family Status],
	Family.[OrganisationId]  AS [OrganisationId],
	Family.[ProviderId]  AS [ProviderId],
	Family.[CentreId]  AS [CentreId],
	Book.BookingStatus AS [Booking Status]
	
FROM [dbo].rpt_Family Family
INNER JOIN [dbo].[rpt_FamilyChild] Child ON Family.FamilyId = Child.FamilyId
INNER JOIN [dbo].[rpt_ChildDetails] cd ON cd.PersonId = child.ChildId
INNER JOIN dbo.rpt_Bookings Book ON Book.FamilyId = Family.FamilyId AND Book.ChildId = Child.ChildId]]></query>
            </view>
            <view id="5d6eb9e9-7055-4ce2-a182-72fe198518ea" name="ChildView" alias="ChildView">
                <query><![CDATA[SELECT
	Family.[CentreName] AS [Centre],
 	Family.[OrganisationName]  AS [Organisation],
	Family.ProviderName AS [Provider],
 	Child.ChildName AS [Child],
	Family.[FamilyName] AS [Family],
	COALESCE(CONVERT(VARCHAR, cd.DateOfBirth, 103), '-') AS [Child DOB],
	'0' AS [Child Age Group],
	ISNULL(cd.KnownAs, '-') AS [Known As],
	ISNULL(cd.CRN, '-') AS CRN,
	ISNULL(cd.[Address], '-') AS [Address],
	ISNULL(cd.Gender, '-') AS Gender,
	ISNULL(cd.AboriginalStatus, '-') AS [Aboriginal],
	ISNULL(cd.CountryOfBirth, '-') AS [Country Of Birth],
	ISNULL(cd.Nationality, '-') AS Nationality,
	ISNULL(cd.CulturalBackground, '-') AS [Cultural Background],
	ISNULL(cd.LanguagesSpoken, '-') AS [Languages Spoken],
	ISNULL(cd.Religion, '-') AS Religion,
	ISNULL(cd.SchoolYear, '-') AS [School Year],
	COALESCE(CONVERT(VARCHAR, cd.SchoolStartDate, 103), '-') AS [School Start Date],
	ISNULL(cd.School, '-') AS [School],

	CONVERT(VARCHAR(MAX), ISNULL((STUFF((SELECT DISTINCT ', ' + CONVERT(NVARCHAR(MAX), book.RoomName) FROM
	dbo.rpt_Bookings book WHERE book.ChildId = Child.ChildId AND book.BookingStatus = 'Approved' FOR XML PATH('')) , 1, 2, '')), '-')) AS Rooms,
	
	 Book.RoomName,

	ISNULL(Relation.RelatedToPersonName, '-') AS Guardian,
	COALESCE(CONVERT(VARCHAR, gd.DateOfBirth, 103), '-') AS [Guardian DOB],
	ISNULL(gd.CRN, '-') AS [Guardian CRN],

	CASE WHEN Adult.AdultType = 'Guardian' THEN ISNULL(gd.[Mobile],'-')
		 WHEN Adult.AdultType = 'Nominee' THEN ISNULL(Nominee.[Mobile],'-')
		 END AS [Mobile],
 	ISNULL(gd.[Phone],'-')  AS [Work],
 	ISNULL(gd.[Home],'-')  AS [Home],
	
 	CASE WHEN Adult.AdultType = 'Guardian' THEN ISNULL(gd.[Email],'-') 
		 WHEN Adult.AdultType = 'Nominee' THEN ISNULL(Nominee.[Email],'-')
		 END AS [Email],
	ISNULL(Family.PriorityContact, '-') AS [Priority Contact Name],
	ISNULL(Family.PriorityContactMobileNumber, '-') AS [Priority Contact No],

	ISNULL(Relation.ContactPriority, '-') AS [Contact Priority],
	ISNULL(Relation.Relationship, '-') AS [Child Relation],

	ISNULL(Adult.AdultType, '-') AS [Person Type],

	
	CASE WHEN Relation.CanAuthoriseSeekMedical = 1 THEN 'Y'
	     WHEN Relation.CanAuthoriseSeekMedical = 0 THEN 'N' 
		 WHEN Relation.CanAuthoriseSeekMedical IS NULL THEN '-' 
		 END AS 'Seek Medical Treatement',
		 
	CASE WHEN Relation.CanAuthoriseAdministerMedical = 1 THEN 'Y'
		 WHEN Relation.CanAuthoriseAdministerMedical = 0 THEN 'N' 
		 WHEN Relation.CanAuthoriseAdministerMedical IS NULL THEN '-' 
		 END AS 'Authorise Medication',
		 
	CASE WHEN Relation.IsEmergencyContact = 1 THEN 'Y'
	     WHEN Relation.IsEmergencyContact = 0 THEN 'N'
		 WHEN Relation.IsEmergencyContact IS NULL THEN '-'
		 END AS 'Act As Emergency Contact',
		 
	CASE WHEN Relation.CanAuthoriseForExcursion = 1 THEN 'Y'
	     WHEN Relation.CanAuthoriseForExcursion = 0 THEN 'N'
		 WHEN Relation.CanAuthoriseForExcursion IS NULL THEN '-'		 
		 END AS 'Excursion Permission',
		 
	CASE WHEN Relation.HasPickupAuthority = 1 THEN 'Y'
	     WHEN Relation.HasPickupAuthority = 0 THEN 'N' 
		 WHEN Relation.HasPickupAuthority IS NULL THEN '-' 
		 END AS 'Dropoff-Pickup',


	COALESCE(CONVERT(VARCHAR, Relation.PickupFromDate, 103), '-') AS [Pickup From Date],
	COALESCE(CONVERT(VARCHAR, Relation.PickupToDate, 103), '-') AS [Pickup To Date],

	CASE WHEN Adult.AdultType = 'Guardian' THEN ISNULL(Adult.AdultFamilyStatus, '-') ELSE ISNULL(Adult.AdultFamilyStatus, '-') END AS [Guardian Status],
	ISNULL(Child.ChildFamilyStatus, '-') AS [Child Status],
	ISNULL(Family.FamilyStatus, '-') AS [Family Status],
	/*Book.BookingDate AS [Booking Date],*/
	ISNULL(gd.KnownAs, '-') AS [Guardian Known As],
	ISNULL(Account.PaymentAccountStatus, '-') AS [Payment Account Status],
	Family.[OrganisationId]  AS [OrganisationId],
	Family.[ProviderId]  AS [ProviderId],
	Family.[CentreId]  AS [CentreId],
	
	CONVERT(VARCHAR(MAX), ISNULL((STUFF((SELECT DISTINCT ', ' + CONVERT(NVARCHAR(MAX), cf.FieldText) +': '+ CASE WHEN CONVERT(NVARCHAR(MAX), cf.FieldValue) = 'true' THEN 'Yes' 	WHEN CONVERT(NVARCHAR(MAX), cf.FieldValue) = 'false' THEN 'No' ELSE CONVERT(NVARCHAR(MAX), cf.FieldValue) END FROM
	dbo.rpt_CustomFields cf WHERE cf.PersonId = cd.PersonId  AND cf.SectionName IS NOT NULL
										 AND cf.OrganisationId = Family.OrganisationId 
										 AND IIF(ISNULL(cf.ProvideId, '') = '', Family.ProviderId, cf.ProvideId) = Family.ProviderId 
										 AND IIF(ISNULL(cf.CentreId, '') = '', Family.CentreId, cf.CentreId) = Family.CentreId
										 FOR XML PATH('')) , 1, 2, '')), '-')) AS [Custom Fields]

 
FROM [dbo].rpt_Family Family
INNER JOIN [dbo].[rpt_FamilyAdult] Adult ON Adult.FamilyId = Family.FamilyId
INNER JOIN [dbo].[rpt_FamilyChild] Child ON Family.FamilyId = Child.FamilyId
INNER JOIN [dbo].[rpt_ChildDetails] cd ON cd.PersonId = child.ChildId
LEFT JOIN [dbo].[rpt_FamilyRelation] Relation ON Family.FamilyId = Relation.FamilyId 
								AND Child.ChildId = Relation.ChildId 
								AND Adult.AdultId = Relation.RelatedToPersonId
LEFT JOIN dbo.rpt_FamilyPaymentAccount Account ON Family.FamilyId = Account.FamilyId 
			AND Adult.AdultId = Account.PaymentAccountPersonId
LEFT JOIN [dbo].[rpt_GuardianDetails] gd ON gd.PersonId = Adult.AdultId
LEFT JOIN [dbo].[rpt_NomineeDetails] Nominee ON Nominee.PersonId = Adult.AdultId
LEFT JOIN dbo.rpt_Bookings Book ON Book.FamilyId = Family.FamilyId AND Book.ChildId = Child.ChildId AND Book.BookingStatus = 'Approved' AND Book.BookingDate BETWEEN CONVERT(DATE, GETUTCDATE()) AND DATEADD(DD, 30, CONVERT(DATE, GETUTCDATE()))
]]></query>
            </view>
        </views>
    </database>
