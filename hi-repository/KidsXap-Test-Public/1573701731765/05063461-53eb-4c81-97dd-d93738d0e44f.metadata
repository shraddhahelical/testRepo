	COALESCE(CONVERT(VARCHAR, cd.AmbulanceSubExpDate, 103), '-') AS [Amb Exp Date],
	ISNULL(cd.PrivateInsuranceName, '-') AS [Private Insurance Provider],
	ISNULL(cd.PrivateInsuranceNumber, '-') AS [Insurance No],
	
	CONVERT(VARCHAR(MAX), ISNULL((STUFF((SELECT DISTINCT ', ' + CONVERT(NVARCHAR(MAX), book.RoomName) FROM
	dbo.rpt_Bookings book WHERE book.ChildId = Child.ChildId AND book.BookingStatus = 'Approved' FOR XML PATH('')) , 1, 2, '')), '-')) AS Rooms,
	
	Book.RoomName,

	ISNULL(Child.ChildFamilyStatus, '-') AS [Child Status],
	ISNULL(Family.FamilyStatus, '-') AS [Family Status],
	/*Book.BookingDate AS [Booking Date],*/
	Family.[OrganisationId]  AS [OrganisationId],
	Family.[ProviderId]  AS [ProviderId],
	Family.[CentreId]  AS [CentreId]
	
	
FROM [dbo].rpt_Family Family
INNER JOIN [dbo].[rpt_FamilyChild] Child ON Family.FamilyId = Child.FamilyId
INNER JOIN [dbo].[rpt_ChildDetails] cd ON cd.PersonId = child.ChildId
LEFT JOIN dbo.rpt_Bookings Book ON Book.FamilyId = Family.FamilyId AND Book.ChildId = Child.ChildId AND Book.BookingStatus = 'Approved' AND Book.BookingDate BETWEEN CONVERT(DATE, GETUTCDATE()) AND DATEADD(DD, 30, CONVERT(DATE, GETUTCDATE()))
 ]]></query>
            </view>
            <view id="48100b60-b0e6-408f-b4fb-13ced8a00548" name="ChildConsentView" alias="ChildConsentView">
                <query><![CDATA[SELECT
	Family.[CentreName] AS [Centre],
 	Family.[OrganisationName]  AS [Organisation],
	Family.ProviderName AS [Provider],
 	Child.ChildName AS [Child],
	Family.[FamilyName] AS [Family],

	CASE WHEN Consent.InEvents = 1 THEN 'Y'
	     WHEN Consent.InEvents = 0 THEN 'N'
		 WHEN Consent.InEvents IS NULL THEN '-' END AS [Events Participation],
	
	ISNULL(CelebrationEvents, '-') AS [Restricated Events],

	
	CASE WHEN Consent.IsSunscreen = 1 THEN 'Y'
	     WHEN Consent.IsSunscreen = 0 THEN 'N'
		 WHEN Consent.IsSunscreen IS NULL THEN '-' END AS [Generic Sunscreen],

	CASE WHEN Consent.IsAllergySunscreen = 1 THEN Consent.SunscreenBrands
	     WHEN Consent.IsAllergySunscreen = 0 THEN '-'
		 WHEN Consent.IsAllergySunscreen IS NULL THEN '-' END AS [Requested Brand],
	'Centre Display' AS [Centre Display1],
	CASE WHEN Consent.DispNamePhoto = 1 THEN 'Y'
	     WHEN Consent.DispNamePhoto = 0 THEN 'N'
		 WHEN Consent.DispNamePhoto IS NULL THEN '-' END AS [Centre Display],
	'Centre Promotion' AS [Centre Promotion1],
	CASE WHEN Consent.UseInCentrePromo = 1 THEN 'Y'
	     WHEN Consent.UseInCentrePromo = 0 THEN 'N'
		 WHEN Consent.UseInCentrePromo IS NULL THEN '-' END AS [Centre Promotion],
	'Group Post'AS [Group Post1],
	CASE WHEN Consent.UseInGroupPost = 1 THEN 'Y'
	     WHEN Consent.UseInGroupPost = 0 THEN 'N'
		 WHEN Consent.UseInGroupPost IS NULL THEN '-' END AS [Group Post],

	CONVERT(VARCHAR(MAX), ISNULL((STUFF((SELECT DISTINCT ', ' + CONVERT(NVARCHAR(MAX), book.RoomName) FROM
	dbo.rpt_Bookings book WHERE book.ChildId = Child.ChildId AND book.BookingStatus = 'Approved' FOR XML PATH('')) , 1, 2, '')), '-')) AS Rooms,
	Book.RoomName,
	Book.BookingDate AS [Booking Date],

	ISNULL(Child.ChildFamilyStatus, '-') AS [Child Status],
	ISNULL(Family.FamilyStatus, '-') AS [Family Status],
	Family.[OrganisationId]  AS [OrganisationId],
	Family.[ProviderId]  AS [ProviderId],
	Family.[CentreId]  AS [CentreId]
	
FROM [dbo].rpt_Family Family
INNER JOIN [dbo].[rpt_FamilyChild] Child ON Family.FamilyId = Child.FamilyId
INNER JOIN [dbo].[rpt_ChildDetails] cd ON cd.PersonId = child.ChildId
LEFT JOIN [dbo].[rpt_ChildConsent] Consent ON Consent.PersonId = child.ChildId
LEFT JOIN dbo.rpt_Bookings Book ON Book.FamilyId = Family.FamilyId AND Book.ChildId = Child.ChildId AND Book.BookingStatus = 'Approved']]></query>
            </view>
