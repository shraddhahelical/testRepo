            <view id="42872a71-0538-409a-a297-5ece83716ca4" name="WaitListCommentProdLevel" alias="WaitListCommentProdLevel">
                <query><![CDATA[ SELECT [confLevelName],
[manage_wl_name],
w.wlId, 
w.OrganisationId, 
w.OrganisationName AS [Organisation], 
wcp.ProviderId,
wcp.ProviderName AS [Provider], 
wcp.CentreId,
wcp.CentreName AS [Centre], 
wcp.Family, 
CASE WHEN wcp.CentrePrefStatus = 1 THEN 'Pending' 
	 WHEN wcp.CentrePrefStatus = 2 THEN 'Offered' 
	 WHEN wcp.CentrePrefStatus = 3 THEN 'Reject' 
	 WHEN wcp.CentrePrefStatus = 4 THEN 'Approved' 
	 WHEN wcp.CentrePrefStatus = 5 THEN 'Archive'
	 WHEN wcp.CentrePrefStatus = 6 THEN 'Confirmed' 
END AS CentrePrefStatus,
w.ChildName AS [Child], 
CONVERT(VARCHAR, w.ChildDOB, 103) AS [Date Of Birth], 
w.AgeAtCareStart AS [Age At Care Start Date], 
wa.PersonName AS [Parent], 
wa.MobileNumber AS [Mobile],
wa.Email AS [Email],
w.wlStatus AS [Application Status], 
w.Tags, 
CONVERT(VARCHAR, w.CreatedOn, 103) AS [Created On], 
w.CreatedBy AS [Created By],
wcb.ProgramName AS [Requested Program], 
CONVERT(VARCHAR, wcb.BookingStartDate, 103) as [Requested Start Date], 
CONVERT(VARCHAR, wcb.BookingEndDate, 103) as [Requested End Date], 
wcb.PreferredDays as [Requested Days],
TRIM(CONVERT(VARCHAR, [value])) [Requested Days-Filter],
CONVERT(VARCHAR, wcb.ofr_OfferDate, 103) AS [Offered Date],
wcb.ofr_ProgramName AS [Offered Program],
CONVERT(VARCHAR, wcb.ofr_BookingStartDate, 103) AS [Offered Booking Start Date], 
CONVERT(VARCHAR, wcb.ofr_BookingEndDate, 103) AS [Offered Booking End Date],  
wcb.ofr_OfferedDays AS [Offered Days],
CONVERT(VARCHAR, wcp.OfferExpiryDate, 103) AS [Offer Expiry Date],
CONVERT(VARCHAR, wcb.OfferActiondDate, 103) AS [Offer Accepted Date],
wcp.[Priority] AS [Preference Number],
CONVERT(VARCHAR(MAX), ISNULL((STUFF((SELECT DISTINCT ', ' + CONVERT(NVARCHAR(MAX), Hear1.HearAboutUs) FROM
[dbo].[rpt_waitListsHearAboutUs] Hear1 WHERE Hear1.wlId = w.wlId FOR XML PATH('')) , 1, 2, '')), '-')) AS [Heard About Us],
Hear2.HearAboutUs AS [Heard About Us-Filter],
wc.[Subject] AS [Subject],
wc.Comment AS [Comments],
CONVERT(DATE, wc.PostedDate) AS [Commented Date],
wc.PostedBy AS [Commented By]
 /*
wcb.OfferActionBy, 
CASE WHEN wcb.OfferAcceptedStatus = 1 THEN 'Pending'
	 WHEN wcb.OfferAcceptedStatus = 2 THEN 'Confirmed'
	 WHEN wcb.OfferAcceptedStatus = 3 THEN 'Completed'
END AS OfferAcceptedStatus,
CASE WHEN wcb.ofr_Action = 0 THEN 'None'
	 WHEN wcb.ofr_Action = 1 THEN 'Confirm Places'
	 WHEN wcb.ofr_Action = 2 THEN 'Offer Days/Program'
	 WHEN wcb.ofr_Action = 3 THEN 'No Vacancies'
END AS ofr_Action
*/
FROM 
[dbo].[rpt_waitLists] w
INNER JOIN [dbo].[rpt_waitListSettings] ws ON ws.OrganisationId = w.OrganisationId AND ws.IsActive = 1
LEFT JOIN [rpt_waitListsAdult] wa ON wa.wlId = w.wlId AND wa.PersonType = 'Guardian' AND ISNULL(wa.IsPrimaryGuardian, 0) = 1
LEFT JOIN [dbo].[rpt_waitListsCentrePref] wcp ON wcp.wlId = w.wlId 
LEFT JOIN [dbo].[rpt_waitListsCentrePrefBookingDetail] wcb ON wcb.wlId = w.wlId AND wcb.CentreId = wcp.CentreId AND wcb.FamilyId = wcp.FamilyId
LEFT JOIN [dbo].[rpt_waitListsHearAboutUs] Hear2 ON Hear2.wlId = w.wlId
LEFT JOIN [dbo].[rpt_waitListsComments] wc ON wc.wlId = w.wlId
CROSS APPLY string_split(wcb.PreferredDays, ',') 
WHERE /*w.OrganisationId = 'd4543528-3bc8-4c51-b6d9-74637787e128' AND w.wlId = 'a98fc6a1-d92c-456d-9c38-f50774ff0eb5'
AND*/ wlStatus NOT IN ('Archived')
AND manage_wl_Id = 'a65b1246-1cf6-4e07-8995-4319bfd9d9fe' /* 'Centrally at provider level' Tag  "Category" : 121 */
AND w.wlStatus IN ('Draft', 'Payment Pending', 'Active', 'Offer Expired', 'Places Offered', 'Offer Rejected', 'Enroled/Extra Days Waiting')]]></query>
            </view>
